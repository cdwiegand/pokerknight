' Receive Packet Function
' =======================
' Written by: Mike
' Function: Receives and takes apart packets from the server.

Private Sub oSocket_PacketReceived(ByVal sPacket As String)
    Dim i As Integer
    Dim cardnum1 As String
    
    If Right(sPacket, 2) = vbCrLf Then sPacket = Left(sPacket, Len(sPacket) - 2)
    
    mbConnected = True
    If Left(sPacket, 2) = "T:" Then
        AddToList Mid(sPacket, 3)
    ElseIf Left(sPacket, 2) = "C:" Then
        If sPacket = "C:CLSP" Then
           LstPlayers.Clear
        End If
        
        If sPacket = "C:Disconnect" Then
            AddToList "** Connection to the Server has been lost. **"
            ' Picture6.Visible = True
            LstPlayers.Clear
            Me.lblStatus = "Offline"
            Me.lblStatus.ForeColor = vbWhite
            Me.lblStatus.BackColor = &H800000
            Command4.Enabled = True
        End If
        If sPacket = "C:PlaySound" Then
            PlaySound App.Path & "\foobar\chimes.wav", 0&, SND_FILENAME Or SND_ASYNC
        End If
        If sPacket = "C:NICK?" Then
            CmdSend "NICK=" & sUserName ' send nick please
        End If
    
        ' New Player List
        If Left(sPacket, 7) = "C:NEWP=" Then
            Me.LstPlayers.AddItem Mid(sPacket, 8)
            Me.lblStatus.Caption = "Online"
            Me.lblStatus.ForeColor = vbWhite
            Me.lblStatus.BackColor = &HFFC0C0
        End If
        
        If Left(sPacket, 7) = "C:DROP=" Then
            For i = 0 To Me.LstPlayers.ListCount - 1
                If Me.LstPlayers.List(i) = Mid(sPacket, 8) Then
                    LstPlayers.RemoveItem i
                    Exit For ' get out of this LOOP - DO NOT CHANGE ME -- chris
                End If
            Next i
        End If
        
        ' Deal Card
            If Left(sPacket, 7) = "C:CARD=" Then
                ' Card has been dealt
       '       PlaySound App.Path & "deal.wav", 0&, SND_FILENAME Or SND_ASYNC
                Dim suit As String
                Dim valcard As String
                suit = Mid(sPacket, 8, 1)
                valcard = Mid(sPacket, 9, 1)
                AddCard suit, valcard
                PaintCards
            End If
    Else
        MsgBox "BAD PACKET: " & sPacket
    End If
End Sub
    

