' Fresh Hand Function
' ===================
' Written by: Chris
' Function: This function will request new cards from the server.
' Bugs: Sometimes deals 6-7 cards instead of 5, blank cards every once in a while

Public Sub FreshHand()
    ' blank em out
    
    Dim i3 As Integer
    Dim i4 As Integer
    
    For i3 = 1 To 5
        myCards(i3) = ""
        backcard(i3 - 1).Visible = True
    Next i3
    ' Blank all the face cards
    blankem

    ' NOW we can ask for new cards
    i4 = 0
    For i4 = 1 To 5
        CmdSend "GETCARD"
    Next i4
End Sub


' Add Card Function
' =================
' Written by: Chris
' Function: This function adds a card to the hand.

Public Sub AddCard(ByVal suit As String, ByVal cardNum As String)
    Dim i As Integer
    Dim ipos As Integer
    
    ' find the next blank card...
    ipos = 0
    For i = 5 To 1 Step -1
        If Trim(myCards(i)) = "" Then ipos = i
    Next i
    
    If ipos = 0 Then
        ' didn't find a blank card...
    Else
        myCards(ipos) = suit & cardNum
    End If
    
    Dim newCards(1 To 5) As String
    Dim s As String
    Dim i2 As Integer
    ' now, sort them
    ipos = 1
    For i = 1 To 13
        s = Mid("234567890JQKA", i, 1) ' get card face value
        For i2 = 1 To 5
            If Mid(myCards(i2), 2, 1) = s Then ' found a card
                newCards(ipos) = myCards(i2)
                ipos = ipos + 1
            End If
        Next i2
    Next i
    
    For i = 1 To 5
        myCards(i) = newCards(i)
    Next i
End Sub


' Paint Cards Function
' ====================
' Written by: Mike
' Function: Display the cards with the card suit and the value.

Public Sub PaintCards()
    Dim ipos As Integer
    Dim suit As String
    Dim damn1 As Integer
    Dim damn2 As String
    Dim damn3 As String
' Damns are just copies of the ipos, value and card values. Seems to work better.
    Dim valcard As String
    ' cardpos 0-4, card array positions
    ' THIS FUNCTION WILL ONLY PAINT THE CARD!
    
    For ipos = 0 To 4
        suit = Left(myCards(ipos + 1), 1)
        valcard = Mid(myCards(ipos + 1), 2, 1)
        backcard(ipos).Visible = False
        cardface(ipos).Visible = True
        frontcard(ipos).Visible = True
        heart(ipos).Visible = False
        clubs(ipos).Visible = False
        Diamond(ipos).Visible = False
        spade(ipos).Visible = False
                  
        ' Display the proper suit
        Select Case suit
             Case "S" ' Spade
                spade(ipos).Visible = True
             Case "D" ' Diamond
                Diamond(ipos).Visible = True
             Case "H" ' Hearts
                heart(ipos).Visible = True
             Case "C" ' Clubs
                clubs(ipos).Visible = True
        End Select
        
    ' Print the card value
    If valcard = "0" Then valcard = "10"
    cardface(ipos).Caption = valcard
    damn2 = valcard
    damn3 = suit
Select Case ipos
    Case 3
    Facecard4 damn2, damn3
    Case 4
    Facecard5 damn2, damn3
End Select
    
    Next ipos
            
End Sub


